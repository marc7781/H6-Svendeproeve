<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MauiApp1.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MauiApp1"
    Shell.FlyoutBehavior="Disabled"
    xmlns:view="clr-namespace:MauiApp1.Views"
    Title="MauiApp1">
    
    <ShellContent
        ContentTemplate="{DataTemplate view:Login}"
        Route="Login" />
    <ShellContent
        ContentTemplate="{DataTemplate view:FindOrder}"
        Route="Ordre" />
    <ShellContent
        ContentTemplate="{DataTemplate view:Signup}"
        Route="Signup" />
    <ShellContent
        ContentTemplate="{DataTemplate view:Profile}"
        Route="Profile" />
    <ShellContent
        ContentTemplate="{DataTemplate view:InspectOrder}"
        Route="InspectOrder" />
    <ShellContent
        ContentTemplate="{DataTemplate view:Settings}"
        Route="Settings" />
    <ShellContent
        ContentTemplate="{DataTemplate view:Ratings}"
        Route="Ratings" />
    <ShellContent
        ContentTemplate="{DataTemplate view:CreateRatings}"
        Route="CreateRatings" />
</Shell>
